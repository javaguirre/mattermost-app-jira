{
"app_id": "com.mattermost.app.jira",
"version": "0.1.0",
"display_name": "Jira",
"icon": "icon.png",
"homepage_url": "https://github.com/mattermost/mattermost-app-jira",
"requested_permissions": [
"act_as_bot",
"act_as_user",
"remote_oauth2",
"remote_webhooks"
],
"requested_locations": [
"/command"
],
"bindings": {
"path": "/bindings",
"expand": {
"acting_user": "all",
"oauth2_user": "all"
}
},
"http": {
"root_url": "http://mattermost-app-jira:4444"
},
"plugin": {
"plugin_id": "com.mattermost.app.jira"
},
"aws_lambda": {
"functions": [
{
"path": "/",
"name": "jira",
"handler": "main",
"runtime": "go1.x"
}
]
},
"remote_webhook_auth_type": "none",
"on_remote_webhook": {
"path": "/webhook",
"expand": {
"oauth2_app": "all"
}
}
}
